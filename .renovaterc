{
  "extends": [
    "config:base",
    "schedule:weekly",
    ":preserveSemverRanges",
    ":disableDevDependencies",
    ":disableDependencyDashboard"
  ],
  "automerge": true,
  "rebaseStalePrs": true,
  "separateMajorMinor": true,
  "groupName": "all dependencies",
  "groupSlug": "all",
  "labels": [
    "deps"
  ],
  "branchPrefix": "deps/",
  "packageRules": [
    {
      "packagePatterns": [
        "*"
      ],
      "groupName": "all dependencies",
      "groupSlug": "all"
    }
  ],
  "ignoreDeps": [
    "@types/node",
    "core-js",
    "cordova-ios",
    "cordova-android",
    "cordova-plugin-googlemaps"
  ]
}